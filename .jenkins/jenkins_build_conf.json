{
  "bionic": {
    "pkg_distribution": "ubuntu",
    "pkg_release": "bionic"
  },
  "onl": {
    "containers": [
      {
        "name": "builder",
        "image": "docker.rtbrick.net/devops/images/rtbrick/debian-stretch-builder",
        "compile_deps": [
          "autoconf-archive",
          "libpam0g-dev",
          "libpam-cracklib",
          "libcjose-dev",
          "rtbrick-json-parser",
          "rtbrick-json-builder",
          "rtbrick-libtacplus-map"
        ],
        "build_commands": [
          "./auto.sh",
          "./configure",
          "make clean",
          "make"
        ],
        "compile_deps_with_dev": true
      }
    ],
    "pkg_group": "rtbrick-onl",
    "pkg_distribution": "debian",
    "pkg_release": "stretch"
  },
  "__defaults__": {
    "project": "rtbrick-libpam-tacplus",
    "version_mmr": {
      "major": 1,
      "minor": 4,
      "rev": 0,
      "label": "",
      "meta": ""
    },
    "containers": [
      {
        "name": "builder",
        "image": "docker.rtbrick.net/devops/images/rtbrick/ubuntu-bionic-builder",
        "compile_deps": [
          "autoconf-archive",
          "libpam0g-dev",
          "libpam-cracklib",
          "libcjose-dev",
          "rtbrick-json-parser",
          "rtbrick-json-builder",
          "rtbrick-libtacplus-map"
        ],
        "build_commands": [
          "./auto.sh",
          "./configure",
          "make clean",
          "make"
        ],
        "compile_deps_with_dev": true
      }
    ],
    "pkg_commands": [
      "mkdir -p /usr/local/share/doc",
      "mkdir -p /usr/share/rtbrick/",
      "mkdir -p /usr/share/rtbrick/tacplus/",
      "mkdir -p /usr/share/rtbrick/tacplus/etc/pam.d/",
      "mkdir -p /usr/share/rtbrick/tacplus/etc/security/",
      "install -o root -g root -m 0640 -D -t /usr/share/rtbrick/tacplus/ jwt-config/usr/share/rtbrick/tacplus/jwt_claims.json",
      "install -o root -g root -m 0640 -D -t /usr/share/rtbrick/tacplus/ jwt-config/usr/share/rtbrick/tacplus/rbac_role_cmds.json",
      "install -o root -g root -m 0640 -D -t /usr/share/rtbrick/tacplus/etc/pam.d/ pam-config/common-account",
      "install -o root -g root -m 0640 -D -t /usr/share/rtbrick/tacplus/etc/pam.d/ pam-config/common-auth",
      "install -o root -g root -m 0640 -D -t /usr/share/rtbrick/tacplus/etc/pam.d/ pam-config/common-password",
      "install -o root -g root -m 0640 -D -t /usr/share/rtbrick/tacplus/etc/pam.d/ pam-config/common-session",
      "install -o root -g root -m 0640 -D -t /usr/share/rtbrick/tacplus/etc/pam.d/ pam-config/common-session-noninteractive",
      "install -o root -g root -m 0640 -D -t /usr/share/rtbrick/tacplus/etc/pam.d/ pam-config/sshd",
      "install -o root -g root -m 0640 -D -t /usr/share/rtbrick/tacplus/etc/pam.d/ pam-config/login",
      "install -o root -g root -m 0640 -D -t /usr/share/rtbrick/tacplus/etc/security/ pam-config/etc/security/group.conf",
      "make \"app_ver=$_ver_str\" install"
    ],
    "pkg_cont": "builder",
    "pkg_name": "rtbrick-libpam-tacplus",
    "pkg_descr": "RtBrick fork of pam_tacplus - extended for full tacacs client solution",
    "pkg_group": "rtbrick-rbfs",
    "pkg_deps": [
      "libpam0g",
      "libpam-cracklib",
      "libpam-pwquality",
      "libcjose0",
      "rtbrick-base-env",
      "rtbrick-json-parser",
      "rtbrick-json-builder",
      "rtbrick-libtacplus-map"
    ],
    "pkg_services": []
  }
}
